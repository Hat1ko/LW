{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "google"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "require-jsdoc": 0,
    "new-cap": 0,
    "max-len": [
      "error",
      {
        "code": 114
      }
    ],
    "no-unused-vars": 0,
    "indent": [
      "error",
      4
    ]
  }
}
